   <talk_paragraph index="n147_mailbox-1-b" image="QN_Npc_Archer_Ishilien.dds" itemindex="">
      <talk_answer link_index="n147_mailbox-1-c" mid="12"><![CDATA[<html>(Continue)</html>]]></talk_answer>
      <talk_question mid="11"><![CDATA[<html>$Isillien's letter$<BR><FONT color=#7e12d3>The guardian of pledge is noble and tough telejia. Elfs who regarded as a guardian of pledge has to walk on the path for protecting Tree of Life.</FONT></html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n147_mailbox-1-c" image="QN_Npc_Archer_Ishilien.dds" itemindex="">
      <talk_answer link_index="n147_mailbox-1-d" mid="14"><![CDATA[<html>(Continue)</html>]]></talk_answer>
      <talk_question mid="13"><![CDATA[<html>$Isillien's letter$<BR><FONT color=#7e12d3>And one of the guardians of pledge will become new Anu Arendel's queen.</FONT></html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n147_mailbox-1-d" image="QN_Npc_Archer_Ishilien.dds" itemindex="">
