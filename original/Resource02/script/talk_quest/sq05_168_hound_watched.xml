   <talk_paragraph index="n016_trader_borin-accepting-a" image="QM_Hound_ice.dds" itemindex="">
      <talk_answer link_index="n016_trader_borin-accepting-b" mid="12"><![CDATA[<html>He did not.</html>]]></talk_answer>
      <talk_question mid="11"><![CDATA[<html>$Blacksmith Borin$<BR>A <FONT color=#a82424>Hound</FONT> appear in my dreams.</html>]]></talk_question>
