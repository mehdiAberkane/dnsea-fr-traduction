      <talk_answer link_index="n064_whilerose_lena-5-n" mid="102"><![CDATA[<html><FONT color=#086171>(Receive Reward)</FONT></html>]]></talk_answer>
      <talk_question mid="101"><![CDATA[<html>$Quest Reward$</html>]]></talk_question>
