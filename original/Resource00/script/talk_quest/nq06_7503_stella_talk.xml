   <talk_paragraph index="n045_soceress_master_stella-accepting" image="" itemindex="">
      <talk_answer link_index="n045_soceress_master_stella-accepting-a" mid="7"><![CDATA[<html>Rodrigo</html>]]></talk_answer>
      <talk_answer link_index="n045_soceress_master_stella-accepting-d" mid="8"><![CDATA[<html>Ithilien</html>]]></talk_answer>
      <talk_answer link_index="n045_soceress_master_stella-accepting-g" mid="9"><![CDATA[<html>Enoch</html>]]></talk_answer>
      <talk_question mid="6"><![CDATA[<html>$Master Sorceress Stella$<BR>Hmm… or is it <FONT color=#a82424>Enoch</FONT>? Annoying Clerics!<BR>Don't you agree?</html>]]></talk_question>
