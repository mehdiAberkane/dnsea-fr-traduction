   <talk_paragraph index="n051_alrento_viscount-2" image="QN_Npc_Noble_Man_Pushibal.dds" itemindex="">
      <talk_answer link_index="n051_alrento_viscount-2-a" mid="23"><![CDATA[<html>What?</html>]]></talk_answer>
